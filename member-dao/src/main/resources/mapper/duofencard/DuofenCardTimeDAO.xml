<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gt.member.dao.duofencard.DuofenCardTimeDAO">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.gt.duofencard.entity.DuofenCardTime">
		<id column="timeId" property="timeId" />
		<result column="cardId" property="cardId" />
		<result column="week" property="week" />
		<result column="holidays" property="holidays" />
		<result column="otherTime" property="otherTime" />
		<result column="otherTimeSet" property="otherTimeSet" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        timeId,cardId, week, holidays, otherTime AS otherTime, otherTimeSet AS otherTimeSet
    </sql>

	<select id="findDuofenCardTimeByCardId" resultMap="BaseResultMap">
		select * from t_duofen_card_time where cardId=#{cardId} limit 1

	</select>

</mapper>
